#ifndef Enumerates_h
#define Enumerates_h

/**
 *  登录类型
 */
typedef NS_ENUM (NSInteger, LoginType) {
    Login = 0,      //普通登录
    ChangeRootView, //登录后，更换根视图控制器
    MinePage,       //我的财富
    SecuritySet,    //安全设置
    LoginTimeOut,   //登录超时
};

/**
 *  资产管理类型
 */
typedef NS_ENUM (NSInteger, AssetManagerType) {
    MyInvestList = 0,   //我的投资
    MyLoanList,         //我的借款
    MyAssigneeList,     //我的受让
    MyTransferList,     //我的转让
};

/**
 *  操作类型
 */
typedef NS_ENUM (NSInteger, ActionType) {
    TYPE_OPEN_ACCOUNT = 1,//开户
    TYPE_INVEST,          //投标
    TYPE_AUCTION,         //竞拍
    TYPE_RECHARGE,        //充值
    TYPE_WITHDRAW,        //提现
    TYPE_BINDING_CARD,    //绑卡
    TYPE_REPAYMENT,       //还款
    TYPE_DEBT_APPLY,      //债权申请（非资金托管）
    TYPE_EXP_EXCHANGE,    //体验金兑换（非资金托管）
    TYPE_JOIN_FINANCE,    //加入理财
};

#endif /* Enumerates_h */
